<?php

require_once 'Testing/Selenium.php';

class Example extends PHPUnit_Framework_TestCase
{
  protected function setUp()
  {
    $this = new Testing_Selenium("*chrome", "http://localhost/")
    $this->open("/hackademic_nextdev/hackademic/admin/");
    $this->click("css=a[title=\"add user\"] > img");
    $this->click("css=a[title=\"add user\"] > img");
    $this->waitForPageToLoad("30000");
    $this->click("css=img.action_image");
    $this->click("css=img.action_image");
    $this->waitForPageToLoad("30000");
    $this->click("link=Users/Classes");
    $this->waitForPageToLoad("30000");
    $this->click("css=img.action_image");
    $this->click("css=img.action_image");
    $this->waitForPageToLoad("30000");
    $this->type("name=username", "hello");
    $this->type("name=username", "hello");
    $this->type("name=full_name", "I am");
    $this->type("name=full_name", "I am");
    $this->type("id=email", "something@somewhere.net");
    $this->type("id=email", "something@somewhere.net");
    $this->type("id=password", "pass");
    $this->type("id=password", "pass");
    $this->type("name=confirmpassword", "pass");
    $this->type("name=confirmpassword", "pass");
    $this->click("id=submit");
    $this->click("id=submit");
    $this->waitForPageToLoad("30000");
    $this->click("link=hello");
    $this->click("link=hello");
    $this->waitForPageToLoad("30000");
    $this->select("name=type", "label=Teacher");
    $this->select("name=type", "label=Teacher");
    $this->click("id=submit");
    $this->click("id=submit");
    $this->waitForPageToLoad("30000");
    $this->click("name=is_activated");
    $this->click("name=is_activated");
    $this->click("id=submit");
    $this->click("id=submit");
    $this->waitForPageToLoad("30000");
    $this->click("link=Users/Classes");
    $this->click("link=Users/Classes");
    $this->waitForPageToLoad("30000");
    $this->click("link=hello");
    $this->click("link=hello");
    $this->waitForPageToLoad("30000");
    $this->click("document.form.is_activated[1]");
    $this->click("document.form.is_activated[1]");
    $this->click("id=submit");
    $this->click("id=submit");
    $this->waitForPageToLoad("30000");
    $this->click("link=Users/Classes");
    $this->click("link=Users/Classes");
    $this->waitForPageToLoad("30000");
    $this->click("link=hello");
    $this->click("link=hello");
    $this->waitForPageToLoad("30000");
    $this->click("id=deletesubmit");
    $this->click("id=deletesubmit");
    $this->waitForPageToLoad("30000");
    $this->click("link=Users/Classes");
  }
}
?>